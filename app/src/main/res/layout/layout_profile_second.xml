<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shop_owner_"
            android:layout_width="93dp"
            android:layout_height="0dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="20dp"
            android:gravity="top"
            android:lineSpacingExtra="5sp"
            android:text="Documents"
            android:textColor="#404040"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="-2.52sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="2dp"
            app:cardCornerRadius="6dp"
            app:cardElevation="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/shop_owner_"
            app:layout_constraintTop_toBottomOf="@+id/shop_owner_">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="20dp">

                                <ImageView
                                    android:id="@+id/imageView4"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/placeholder"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="16dp"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                                    app:layout_constraintEnd_toStartOf="@+id/button2"
                                    app:layout_constraintStart_toEndOf="@+id/imageView4"
                                    app:layout_constraintTop_toTopOf="@+id/imageView4">

                                        <TextView
                                            android:id="@+id/gst_certifi"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="top"
                                            android:lineSpacingExtra="3sp"
                                            android:singleLine="true"
                                            android:text="GST Certificate"
                                            android:textColor="#404040"
                                            android:textSize="16sp"
                                            android:translationY="-1.67sp" />

                                        <TextView
                                            android:id="@+id/not_found"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentLeft="true"
                                            android:layout_alignParentTop="true"
                                            android:layout_alignParentRight="true"
                                            android:layout_alignParentBottom="true"
                                            android:gravity="top"
                                            android:text="Not Found"
                                            android:textColor="#F06979" />
                                </LinearLayout>

                                <Button
                                    android:id="@+id/button2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:background="@drawable/upload_button_bg"
                                    android:text="UPLOAD"
                                    android:textColor="#80A0FF"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="20dp">

                                <ImageView
                                    android:id="@+id/imageView41"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/placeholder"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="16dp"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView41"
                                    app:layout_constraintEnd_toStartOf="@+id/button3"
                                    app:layout_constraintStart_toEndOf="@+id/imageView41"
                                    app:layout_constraintTop_toTopOf="@+id/imageView41">

                                        <TextView
                                            android:id="@+id/gst_certifi1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="top"
                                            android:lineSpacingExtra="3sp"
                                            android:singleLine="true"
                                            android:text="PAN - Front "
                                            android:textColor="#404040"
                                            android:textSize="16sp"
                                            android:translationY="-1.67sp" />

                                        <TextView
                                            android:id="@+id/not_found1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentLeft="true"
                                            android:layout_alignParentTop="true"
                                            android:layout_alignParentRight="true"
                                            android:layout_alignParentBottom="true"
                                            android:gravity="top"
                                            android:text="Not Found"
                                            android:textColor="#F06979" />
                                </LinearLayout>

                                <Button
                                    android:id="@+id/button3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:background="@drawable/upload_button_bg"
                                    android:text="UPLOAD"
                                    android:textColor="#80A0FF"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>


                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="20dp">

                                <ImageView
                                    android:id="@+id/imageView42"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/placeholder"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="16dp"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView42"
                                    app:layout_constraintEnd_toStartOf="@+id/button4"
                                    app:layout_constraintStart_toEndOf="@+id/imageView42"
                                    app:layout_constraintTop_toTopOf="@+id/imageView42">

                                        <TextView
                                            android:id="@+id/gst_certifi2"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="top"
                                            android:lineSpacingExtra="3sp"
                                            android:singleLine="true"
                                            android:text="PAN - Back"
                                            android:textColor="#404040"
                                            android:textSize="16sp"
                                            android:translationY="-1.67sp" />

                                        <TextView
                                            android:id="@+id/not_found12"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentLeft="true"
                                            android:layout_alignParentTop="true"
                                            android:layout_alignParentRight="true"
                                            android:layout_alignParentBottom="true"
                                            android:gravity="top"
                                            android:text="Not Found"
                                            android:textColor="#F06979" />
                                </LinearLayout>

                                <Button
                                    android:id="@+id/button4"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:background="@drawable/upload_button_bg"
                                    android:text="UPLOAD"
                                    android:textColor="#80A0FF"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>


                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="20dp">

                                <ImageView
                                    android:id="@+id/imageView43"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:src="@drawable/placeholder"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginEnd="16dp"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView43"
                                    app:layout_constraintEnd_toStartOf="@+id/button5"
                                    app:layout_constraintStart_toEndOf="@+id/imageView43"
                                    app:layout_constraintTop_toTopOf="@+id/imageView43">

                                        <TextView
                                            android:id="@+id/gst_certifi3"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="top"
                                            android:lineSpacingExtra="3sp"
                                            android:singleLine="true"
                                            android:text="Owner aadhar card"
                                            android:textColor="#404040"
                                            android:textSize="16sp"
                                            android:translationY="-1.67sp" />

                                        <TextView
                                            android:id="@+id/not_found123"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentLeft="true"
                                            android:layout_alignParentTop="true"
                                            android:layout_alignParentRight="true"
                                            android:layout_alignParentBottom="true"
                                            android:gravity="top"
                                            android:text="Not Found"
                                            android:textColor="#F06979" />
                                </LinearLayout>

                                <Button
                                    android:id="@+id/button5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:background="@drawable/upload_button_bg"
                                    android:text="UPLOAD"
                                    android:textColor="#80A0FF"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>
        </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>

